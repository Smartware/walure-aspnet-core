@using WebApp.Models;
@model List<Person>

<h1>Hello Fejiro  -  @ViewBag.date</h1>

<table border="1">
    <thead>
        <tr>
            <td>Last Name</td>
            <td>First Name</td>
            <td>Age</td>
        </tr>
    </thead>
    @foreach (Person item in Model)
    {
        <tr>
            <td>@item.LastName</td>
            <td>@item.FirstName</td>
            <td>@item.Age</td>
        </tr>

    }

</table>